                  style="push">
            </command>
            <command
                  commandId="RAP-FirstProject.addProject"
                  icon="icons/addfolder.png"
                  style="push">
            </command>
            <command
                  commandId="RAP-FirstProject.version"
                  icon="icons/version.png"
                  style="push">
            </command>
            <command
                  commandId="RAP-FirstProject.save"
                  icon="icons/save_as.png"
                  style="push">
            </command>
            <command
                  commandId="RAP-FirstProject.warning"
                  icon="icons/warning.png"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:RAP-FirstProject.project">
         <command
               commandId="RAP-FirstProject.addProject"
               icon="icons/project.png"
               label="New project"
               style="push">
         </command>
         <command
               commandId="RAP-FirstProject.addVersion"
               icon="icons/version.png"
               label="New version"
               style="push">
         </command>
         <command
               commandId="RAP-FirstProject.save"
               icon="icons/save_as.png"
               label="Save as"
               style="push">
         </command>
         <command
               commandId="RAP-FirstProject.delete"
               icon="icons/delete.png"
               label="Delete"
               style="push">
