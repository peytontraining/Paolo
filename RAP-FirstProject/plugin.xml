<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="mailapp.entrypoints"
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            path="/firstproject"
            applicationId="RAP-FirstProject.firstproject"
            id="mailapp.entrypoint">
      </entrypoint>
   </extension>
   <extension
         id="RAP-FirstProject.firstproject"
         point="org.eclipse.core.runtime.applications">
      <application
            thread="main"
            cardinality="singleton-global"
            visible="true">
         <run
               class="application.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="application.Perspective"
            icon="icons/project.png"
            id="RAP-FirstProject.projectPerspective"
            name="Project">
      </perspective>
      <perspective
            class="application.TemplatePerspective"
            icon="icons/template.png"
            id="RAP-FirstProject.templatePerspective"
            name="Template">
      </perspective>
      <perspective
            class="application.SystemPerspective"
            icon="icons/system.png"
            id="RAP-FirstProject.systemPerspective"
            name="System">
      </perspective>
      <perspective
            class="application.UserPerspective"
            icon="icons/user.png"
            id="RAP-FirstProject.userPerspective"
            name="User">
      </perspective>
      <perspective
            class="application.LogoutPerspective"
            icon="icons/logout.png"
            id="RAP-FirstProject.logoutPerspective"
            name="Logout">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="view.project.TreeViewPart"
            icon="icons/project.png"
            id="RAP-FirstProject.project"
            name="Project"
            restorable="true">
      </view>
      <view
            class="view.project.DeviceTableView"
            icon="icons/device.png"
            id="RAP-FirstProject.deviceSelection"
            name="Device"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="view.project.VersionPropertiesView"
            id="RAP-FirstProject.VersionPropertiesView"
            name="VersionProperties"
            restorable="true">
      </view>
      <view
            class="view.template.AreasTemplatesView"
            icon="icons/areas.png"
            id="RAP-FirstProject.areas"
            name="Areas"
            restorable="true">
      </view>
      <view
            class="view.project.VersionPropertiesView"
            icon="icons/homeworks-controller.png"
            id="RAP-FirstProject.drivers"
            name="Drivers"
            restorable="true">
      </view>
      <view
            class="view.project.VersionPropertiesView"
            icon="icons/service.png"
            id="RAP-FirstProject.services"
            name="Service"
            restorable="true">
      </view>
      <view
            class="view.project.VersionPropertiesView"
            icon="icons/scene.png"
            id="RAP-FirstProject.scenes"
            name="Scenes"
            restorable="true">
      </view>
      <view
            class="view.project.VersionPropertiesView"
            icon="icons/rule.png"
            id="RAP-FirstProject.rules"
            name="Rules"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="view.project.ProjectPropertiesView"
            id="RAP-FirstProject.projectPropertiesViewPart"
            name="ProjectProperties"
            restorable="true">
      </view>
      <view
            class="view.template.DevicesTemplatesTableView"
            icon="icons/template.png"
            id="RAP-FirstProject.devicesTemplatesView"
            name="DevicesTemplates"
            restorable="true">
      </view>
      <view
            class="view.template.DetailDeviceTemplateView"
            id="RAP-FirstProject.detailDeviceTemplateView"
            name="Detail Device Template"
            restorable="true">
      </view>
      <view
            class="view.dialog.NewDeviceView"
            id="RAP-FirstProject.newDevice"
            name="New Device"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="view.project.DetailDeviceView"
            id="RAP-FirstProject.detailDeviceView"
            name="Detail Device"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="handlers.DefaultHandler"
            id="RAP-FirstProject.export"
            name="Export">
      </command>
      <command
            defaultHandler="handlers.DefaultHandler"
            id="RAP-FirstProject.deploy"
