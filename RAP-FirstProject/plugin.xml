            name="Add project">
      </command>
      <command
            defaultHandler="handlers.AddDeviceTemplateHandler"
            id="RAP-FirstProject.selectTemplate"
            name="Select template">
      </command>
      <command
            defaultHandler="handlers.DefaultHandler"
            id="RAP-FirstProject.version"
            name="Version">
      </command>
      <command
            defaultHandler="handlers.SaveAsVersionHandler"
            id="RAP-FirstProject.save"
            name="Save">
      </command>
      <command
            defaultHandler="handlers.DefaultHandler"
            id="RAP-FirstProject.warning"
            name="Warning">
      </command>
      <command
            defaultHandler="handlers.DeleteVersionHandler"
            id="RAP-FirstProject.delete"
            name="Delete version">
      </command>
      <command
            defaultHandler="handlers.NewVersionHandler"
            id="RAP-FirstProject.addVersion"
            name="Add version">
      </command>
      <command
            defaultHandler="handlers.SaveDeviceHandler"
            id="RAP-FirstProject.saveDevice"
            name="Save device">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.trim.command1">
         <toolbar
               id="toolbar1:org.eclipse.ui.main.toolbar ">
            <command
                  commandId="RAP-FirstProject.export"
                  icon="icons/export.png"
                  mode="FORCE_TEXT"
                  style="push">
            </command>
            <command
