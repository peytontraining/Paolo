                  commandId="RAP-FirstProject.helplink"
                  icon="icons/help.png"
                  mode="FORCE_TEXT"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:view.TreeViewPart">
